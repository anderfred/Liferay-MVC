<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN"
        "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="com.anderfred">
    <namespace>testApi</namespace>
    <entity name="Vacancy" local-service="true" remote-service="false" table="vacancy">
        <column name="id" type="int" primary="true"></column>
        <column name="publishedDate" type="Date"></column>
        <column name="employer" type="String"></column>
        <column name="text" type="String"></column>
        <column name="salary" type="String"></column>
        <column name="area" type="int"></column>
        <column name="spec" type="int"></column>
    </entity>
</service-builder>