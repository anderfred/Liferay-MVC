<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
    <import class="com.anderfred.model.Vacancy"/>
    <class name="com.anderfred.model.impl.VacancyImpl" table="vacancy">
        <id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" column="id_" name="id" type="int">
            <generator class="assigned"/>
        </id>
        <property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="publishedDate"
                  type="com.liferay.portal.dao.orm.hibernate.StringType"/>
        <property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="employer"
                  type="com.liferay.portal.dao.orm.hibernate.StringType"/>
        <property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" column="text_" name="text"
                  type="com.liferay.portal.dao.orm.hibernate.StringType"/>
        <property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="salary"
                  type="com.liferay.portal.dao.orm.hibernate.StringType"/>
    </class>
</hibernate-mapping>